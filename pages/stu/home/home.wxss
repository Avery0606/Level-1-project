.main-container{
  display: flex;
  flex-wrap:wrap;
  justify-content: space-between;
  align-items: center;
  align-content: space-around;
  background-color: #ECECEC;
  height:92vh;
}
.main-container navigator{
  border-radius:10px ;
  border: 1px solid black;
  width:35%;
  height:20vh;
  margin-left: 7vw;
  margin-right: 7vw;
  margin-top:1vh;
  margin-bottom: 1vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: white;
}
.main-container navigator image{
  width:100rpx;
  height:100rpx;
}
.main-container navigator view{
  font-family: "微软雅黑";
  margin-top:1vh;
  font-size:4vw;
}